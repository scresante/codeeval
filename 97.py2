#!/usr/bin/python
from sys import argv
try:
    f = argv[1]
except:
    f = 'tests/97'
data = open(f,'r').read().splitlines()

for line in data:
    if not line:
        continue
    s,i = line.split('|')
    i = i.strip().split(' ')
    i = [(int(q) - 1) for q in i]
    print(''.join([s[q] for q in i]))
