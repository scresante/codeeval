#!/usr/bin/python
from sys import argv
try:
  f = argv[1]
except:
  f = 'tests/16'
data = open(f,'r').read().splitlines()

def tobin(numstr):
  return '{0:b}'.format(int(numstr))

for line in data:
  if not line:
    continue
#  print line
#  print tobin(line)
  count=0
  for s in tobin(line):
    if s=='1':
      count = count + 1
  print count
